app: Bike
-
move up: key(ctrl-cmd-up)
move down: key(ctrl-cmd-down)
focus in: key(alt-cmd-right)
focus out: key(alt-cmd-left)
collapsing: key(cmd-9)
expanding: key(cmd-0)
collapse all: key(ctrl-cmd-9)
expand all: key(ctrl-cmd-0)
table: key(ctrl-shift-tab)
tabber | tipsy: key(ctrl-tab)
typewriter: key(alt-cmd-t)